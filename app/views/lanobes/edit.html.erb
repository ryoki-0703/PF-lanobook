<h1>編集画面</h1>
<%= form_with model:@lanobe, local: true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>説明</h4>
  <%= f.text_area :introduction %>
  <%= attachment_image_tag @lanobe, :image, :fill, 60, 60, fallback: "no_image.jpg"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.attachment_field :image, placeholder: "プロフィール画像" %>
  <%= f.submit '保存' %>
<% end %>